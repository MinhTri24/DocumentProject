@model DocumentProject.Models.Category?

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<button type="button"><a asp-action="Index">Back</a></button>
<button type="button"><a asp-action="Delete" asp-route-id="@Model.Id">Delete</a></button>
<button type="button"><a asp-action="Update" asp-route-id="@Model.Id">Update</a></button>
@{
        <div class="card">
            <div class="container">
            <h4><b><a>@Model.Id</a></b></h4>
            <h5><b><a>@Model.CategoryName</a></b></h5>
            </div>
        </div>
}